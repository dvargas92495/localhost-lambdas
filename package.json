    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.28",
    "eslint": "^7.27.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@hapi/hapi": "^20.1.3",
