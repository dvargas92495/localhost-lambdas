{
  "name": "localhost-lambdas",
  "description": "Description for localhost-lambdas",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "npm t",
    "build": "ncc build src/index.ts -o dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build",
