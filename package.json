    "pretest": "npm run lint",
    "test": "jest --config jestconfig.json"
  },
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.27.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.2"
  }
}
