  "version": "0.5.5",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "cross-env NODE_ENV=test npm t",
    "build": "esbuild src/index.ts --outfile=dist/index.js --bundle --platform=node --external:ngrok",
